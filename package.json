{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "este es un curso muy diveertido",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode development",
    "build:external": "webpack --config ./external/webpack.config.js --mode development",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js --mode development",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js --mode development",
    "build:multi": "webpack --config ./multiples-entry-points/webpack.config.js --mode development",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js --mode development",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js --mode development",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js --mode development",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js --mode development",
    "build:video": "webpack --config ./file-loader-video/webpack.config.js --mode development",
    "build:json": "webpack --config ./json-loader/webpack.config.js --mode development",
    "build:react": "webpack --config ./react/webpack.config.js --mode development",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js --mode development",
    "build:stylus": "webpack --config ./stylus-loader/webpack.config.js --mode development",
    "build:less": "webpack --config ./less-loader/webpack.config.js --mode development",
    "build:postcss": "webpack --config ./postcss-loader/webpack.config.js --mode development",
    "build:prevent": "webpack --config ./prevent-duplication/webpack.config.js --mode development",
    "build:vendor": "webpack --config ./vendor/webpack.config.js --mode development",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js --mode development",
    "build:dll:src": "webpack --config ./dll/webpack.config.js --mode development",
    "build:dynamic:dll": "webpack --config ./dynamic-imports/webpack.dll.config.js --mode development",
    "build:dynamic": "webpack --config ./dynamic-imports/webpack.config.js --mode development"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@LeonidasEsteban",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.7",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "json-loader": "0.5.7",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "mini-css-extract-plugin": "0.4.0",
    "nib": "1.1.2",
    "node-sass": "4.5.3",
    "postcss": "6.0.13",
    "postcss-cssnext": "3.0.2",
    "postcss-loader": "2.0.8",
    "rupture": "0.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.2",
    "url-loader": "0.6.2",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.1.4"
  },
  "dependencies": {
    "react": "16.0.0",
    "react-dom": "16.0.0"
  }
